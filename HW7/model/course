const mongoose = require('mongoose');
//course  schema and model


const cs = mongoose.Schema({
    courses_count:Number,
    path_status:Boolean,
    learning_goals:[String],
    path_hours:Number,
    publishing_status:Boolean,
    //_id:String, given by mongodb  by default
    color_hex:String,
    level:String,
    description:String,
    requirements:[String] ,
    url_id:String,
    title:String,
  
},{timestamps:true});

module.exports = mongoose.model('course',cs);